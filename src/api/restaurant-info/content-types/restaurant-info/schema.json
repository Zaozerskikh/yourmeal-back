{
  "kind": "collectionType",
  "collectionName": "restaurant_infos",
  "info": {
    "singularName": "restaurant-info",
    "pluralName": "restaurant-infos",
    "displayName": "RestaurantInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true,
      "unique": false
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "openingTime": {
      "type": "time",
      "required": true
    },
    "closingTime": {
      "type": "time",
      "required": true
    },
    "rate": {
      "type": "float",
      "required": false
    },
    "foodPositions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::food-position.food-position"
    },
    "menuSectionsTags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::menu-section-tag.menu-section-tag"
    },
    "mediumCheck": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::medium-check.medium-check"
    },
    "positionsOnHome": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::food-position.food-position"
    },
    "restaurantInfoTags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::restaraunt-info-tag.restaraunt-info-tag"
    }
  }
}
